{
  "name": "[[name]]",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "npm run lint && vue-tsc --noEmit && vite build",
    "format": "prettier --write resources/js/**/*.{ts,tsx,js,jsx,vue}",
    "format:check": "prettier --check resources/js/**/*.{ts,tsx,js,jsx,vue}",
    "lint": "eslint ./resources/js/**/*.{ts,tsx,js,jsx,vue}",
    "lint:ignore-warnings": "eslint ./resources/js/**/*.{ts,tsx,js,jsx,vue} --quiet",
    "lint:fix": "eslint ./resources/js/**/*.{ts,tsx,js,jsx,vue} --fix"
  },
  "dependencies": {
    "tailwindcss": "^4.0.12",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@floating-ui/vue": "^1.1.6",
    "@heroicons/vue": "^2.2.0",
    "@tailwindcss/postcss": "^4.0.12",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vueuse/core": "^13.0.0",
    "ahsandevs": "^0.1.19",
    "ahsandevs-vite-plugin": "^0.2.0",
    "cooltipz-css": "^2.3.1",
    "eslint": "9.22.0",
    "eslint-plugin-vue": "^10.0.0",
    "lucide-vue-next": "^0.479.0",
    "pinia": "^3.0.1",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "thetheme": "^0.3.4",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.26.1",
    "vite": "^6.2.1",
    "vue-eslint-parser": "^10.1.1",
    "vue-router": "^4.5.0",
    "vue-tsc": "^2.2.8"
  }
}
