{
  "name": "[[name]]",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "npm run lint && vue-tsc --noEmit && vite build",
    "format": "prettier --write resources/js/**/*.{ts,tsx,js,jsx,vue}",
    "format:check": "prettier --check resources/js/**/*.{ts,tsx,js,jsx,vue}",
    "lint": "eslint ./resources/js/**/*.{ts,tsx,js,jsx,vue}",
    "lint:ignore-warnings": "eslint ./resources/js/**/*.{ts,tsx,js,jsx,vue} --quiet",
    "lint:fix": "eslint ./resources/js/**/*.{ts,tsx,js,jsx,vue} --fix"
  },
  "dependencies": {
    "tailwindcss": "^4.0.13",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.0.13",
    "@vitejs/plugin-vue": "^5.2.1",
    "ahsandevs-vite-plugin": "^0.2.0",
    "eslint": "9.22.0",
    "eslint-plugin-vue": "^10.0.0",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.26.1",
    "vite": "^6.2.1",
    "vue-eslint-parser": "^10.1.1",
    "vue-tsc": "^2.2.8"
  }
}
